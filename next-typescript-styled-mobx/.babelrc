{
  "presets": [["next/babel"]],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@utils": "./src/utils",
          "@interface": "./src/interface",
          "@hooks": "./src/hooks",
          "@stores": "./src/stores"
        }
      }
    ],
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}
